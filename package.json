{
  "name": "@ivuorinen/commitlint-config",
  "version": "0.3.12",
  "type": "module",
  "description": "ivuorinen's shareable configuration for commitlint.",
  "author": {
    "name": "Ismo Vuorinen",
    "url": "https://github.com/ivuorinen"
  },
  "bugs": {
    "url": "https://github.com/ivuorinen/base-configs-commitlint/issues"
  },
